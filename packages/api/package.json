{
  "name": "@tribal-ehr/api",
  "version": "1.0.0",
  "description": "Tribal EHR API - Main backend service for the Electronic Health Record system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --notify false src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --passWithNoTests",
    "migrate": "knex migrate:latest --knexfile knexfile.ts",
    "migrate:make": "knex migrate:make --knexfile knexfile.ts",
    "seed": "knex seed:run --knexfile knexfile.ts",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "cookie-parser": "^1.4.6",
    "pg": "^8.11.3",
    "knex": "^3.1.0",
    "ioredis": "^5.3.2",
    "amqplib": "^0.10.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.0",
    "zod": "^3.22.4",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.5",
    "express-rate-limit": "^7.1.5",
    "@tribal-ehr/shared": "*",
    "@tribal-ehr/hl7-engine": "*"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/cookie-parser": "^1.4.6",
    "@types/pg": "^8.10.9",
    "@types/amqplib": "^0.10.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/node": "^20.11.0",
    "ts-node-dev": "^2.0.0",
    "ts-node": "^10.9.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.0",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  }
}
